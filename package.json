{
  "name": "rerobots",
  "version": "0.1.1",
  "description": "JS client library for rerobots",
  "repository": {
    "type": "git",
    "url": "https://github.com/rerobots/js.git"
  },
  "author": "rerobots, Inc. <q@rerobots.net>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "webpack",
    "build:release": "webpack --mode production",
