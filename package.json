    "test": "jest"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*.d.ts",
    "yarn.lock"
  ],
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "webpack": "^5.97.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {}
}
